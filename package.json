{
	"name": "cdk-s3www",
	"version": "0.1.0",
	"bin": {
		"cdk-s3www": "bin/cdk-s3www.js"
	},
	"scripts": {
		"format": "biome format --write . && dprint fmt && textlint --fix \"**/*.md\"",
		"check": "biome check --write . && dprint check",
		"bootstrap": "cdk bootstrap",
		"list": "cdk list",
		"synth": "cdk synth",
		"deploy": "cdk deploy --outputs-file outputs.tmp.json",
		"deploy:hotswap": "cdk deploy --hotswap --outputs-file outputs.tmp.json",
		"diff": "cdk diff",
		"destroy": "cdk destroy"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "22.7.9",
		"aws-cdk": "2.170.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"typescript": "~5.6.3"
	},
	"dependencies": {
		"aws-cdk-lib": "2.170.0",
		"constructs": "^10.4.2"
	}
}
